@@ -2095,8 +2220,12 @@ int main(int argc, char *argv[])
 					total += wiped;
 			}
 
-			if (opts.unused) {
-				wiped = wipe_unused(vol, byte, act);
+			if (opts.unused || opts.unused_fast) {
+				if (opts.unused_fast)
+					wiped = wipe_unused_fast(vol, byte,
+								act);
+				else
+					wiped = wipe_unused(vol, byte, act);
 				if (wiped < 0)
 					goto umount;
 				else
diff --git a/release/src-rt-6.x.4708/router/ntfs-3g/ntfsprogs/ntfswipe.h b/release/src-rt-6.x.4708/router/ntfs-3g/ntfsprogs/ntfswipe.h
