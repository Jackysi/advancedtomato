 	int result = 1;
 	int flags = 0;
+	int res;
 	int i, j;
 	enum action act = act_info;
 
 	ntfs_log_set_handler(ntfs_log_handler_outerr);
 
-	if (!parse_options(argc, argv))
-		return 1;
+	res = parse_options(argc, argv);
+	if (res >= 0)
+		return (res);
 
