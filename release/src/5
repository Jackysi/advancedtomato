-			return geo.sectors;
+	if (dev->d_sectors_per_track == -1) {
+		if (ntfs_device_get_geo(dev) == -1)
+			return -1;
+		if (dev->d_sectors_per_track == -1) {
+			errno = EINVAL;
+			return -1;
 		}
 	}
-#else
-	errno = EOPNOTSUPP;
-#endif
-	return -1;
+	return dev->d_sectors_per_track;
 }
 
 /**
